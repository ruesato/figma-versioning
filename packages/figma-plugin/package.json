{
  "name": "@figma-versioning/figma-plugin",
  "version": "0.0.0",
  "description": "Figma plugin for design versioning and changelog",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss --input ./src/input.css --output ./src/output.css",
    "build:js": "build-figma-plugin --typecheck --minify",
    "dev": "npm run build:css && concurrently npm:watch:css npm:watch:js",
    "watch:css": "tailwindcss --input ./src/input.css --output ./src/output.css --watch",
    "watch:js": "build-figma-plugin --typecheck --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src"
  },
  "keywords": [
    "figma",
    "figma-plugin",
    "versioning"
  ],
  "author": "Ryan Uesato",
  "license": "MIT",
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "figma-versioning",
    "name": "Figma Versioning",
    "main": "src/main.ts",
    "ui": "src/ui.tsx",
    "build": "build"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.3",
    "@create-figma-plugin/utilities": "^4.0.3",
    "preact": "^10.26.1"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@figma/plugin-typings": "^1.103.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
