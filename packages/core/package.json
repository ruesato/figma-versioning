{
  "name": "@figma-versioning/core",
  "version": "0.0.0",
  "description": "Core library for Figma versioning and changelog functionality",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "figma",
    "versioning",
    "changelog"
  ],
  "author": "Ryan Uesato",
  "license": "MIT",
  "devDependencies": {
    "@vitest/ui": "^4.0.18",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
