{
  "name": "figma-versioning",
  "version": "0.0.0",
  "private": true,
  "description": "Figma design changelog and versioning system",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "pnpm --parallel --filter './packages/*' dev",
    "build": "pnpm --filter './packages/*' build",
    "test": "pnpm --filter './packages/*' test",
    "lint": "pnpm --filter './packages/*' lint",
    "typecheck": "pnpm --filter './packages/*' typecheck"
  },
  "keywords": [
    "figma",
    "versioning",
    "changelog",
    "design"
  ],
  "author": "Ryan Uesato",
  "license": "MIT"
}
